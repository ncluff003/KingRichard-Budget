{
  "name": "kingrichard-budget",
  "version": "1.0.0",
  "description": "budgeting like royalty",
  "main": "index.js",
  "scripts": {
    "BundleJS": "webpack --config ./KingRichard-Budget/webpack.config.js --mode production",
    "CompileSass": "sass ./Public/SCSS:./Public/DIST/CSS Public/SCSS/Main/main.scss:./Public/DIST/CSS/kingrichard.comp.css --style=expanded --embed-sources",
    "PrefixCSS": "postcss --use=autoprefixer -b \"last 20 versions\" ./Public/DIST/CSS/kingrichard.comp.css -o ./Public/DIST/CSS/kingrichard.prefix.css",
    "CompressCSS": "sass ./Public/DIST/CSS/kingrichard.prefix.css:./Public/DIST/CSS/kingrichard.css --style=compressed --embed-sources",
    "BuildCSS": "npm-run-all CompileSass PrefixCSS CompressCSS ",
    "BundleDevJS": "webpack serve --open --config ./webpack.config.js --mode development",
    "LocalServer": "nodemon Backend_JS/Server.js -e ejs,js,css,html,jpg,png,scss",
    "WatchSCSS": "onchange '**/*.scss' -- npm-run-all BuildCSS",
    "WatchJS": "onchange '**/*.js' -- npm-run-all BundleDevJS",
    "Start": "npm-run-all --parallel LocalServer WatchSCSS WatchJS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncluff003/KingRichard-Budget.git"
  },
  "keywords": [
    "Budget",
    "Royalty",
    "Robin",
    "Hood",
    "King",
    "Richard"
  ],
  "author": "Nathan Cluff",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ncluff003/KingRichard-Budget/issues"
  },
  "homepage": "https://github.com/ncluff003/KingRichard-Budget#readme",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.8",
    "@csstools/postcss-sass": "^4.0.0",
    "autoprefixer": "^10.3.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "concat": "^1.0.3",
    "dart-sass": "^1.25.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
