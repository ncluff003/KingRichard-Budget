html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    link(rel='shortcut icon' href='KingRichard_FavIcon.ico' type='image/x-icon')
    title King Richard
    meta(name='description' content='')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    link(rel='stylesheet' href='/DIST/CSS/kingrichard.css')
    script(type='module' src='/DIST/Bundle.js' defer)
  body
    //
      [if lt IE 7]>
      <p class="browsehappy">
